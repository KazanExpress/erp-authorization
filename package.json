{
  "name": "@kazanexpress/erp-authorization",
  "main": "./dist/ErpAuth.common.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve src/app/index.ts",
    "build": "vue-cli-service build src/app/index.ts",
    "build-bundle": "vue-cli-service build --target lib --name ErpAuth ./src/pages/index.ts ./dist",
    "lint": "vue-cli-service lint src/app/index.ts",
    "apollo-schema:dev": "npx apollo service:download --endpoint=https://gateway.erp.dev.cluster.kznexpess.com/ src/shared/api-apollo/models/schema.json && npm run apollo-possible-types",
    "apollo-schema:prod": "npx apollo service:download --endpoint=https://gateway.erp.dev.cluster.kznexpess.com/ src/shared/api-apollo/models/schema.json && npm run apollo-possible-types",
    "apollo-types": "npx apollo codegen:generate src/shared/api-apollo/models/types.ts --outputFlat --localSchemaFile=src/shared/api-apollo/models/schema.json --target=typescript --tagName=gql --globalTypesFile='src/shared/api-apollo/models/global-types.ts'",
    "apollo-possible-types": "ts-node src/shared/api-apollo/models/possibleTypesGenerator.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@vue/apollo-composable": "^4.0.0-alpha.18",
    "@intlify/unplugin-vue-i18n": "^0.7.0",
    "@vuelidate/core": "^2.0.0-alpha.19",
    "@vuelidate/validators": "^2.0.0-alpha.17",
    "babel-plugin-import": "^1.13.3",
    "core-js": "^3.15.1",
    "enhanced-resolve": "5.8.2",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "ky": "^0.28.5",
    "lodash.debounce": "^4.0.8",
    "maska": "^1.4.2",
    "parakeet-mapper": "^2.4.1",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vue/cli-plugin-babel": "^4.5.17",
    "@vue/cli-plugin-eslint": "~4.5.17",
    "@vue/cli-plugin-typescript": "^4.5.17",
    "@vue/cli-service": "^4.5.17",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^7.11.1",
    "prettier": "^2.3.1",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.13.1",
    "stylelint-plugin-stylus": "^0.11.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.4",
    "vue": "^3.2.21",
    "vue-eslint-parser": "^7.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.svg"
  ]
}
